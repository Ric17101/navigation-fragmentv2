                android:text="Text" />
        </TableRow>

        <TableRow
            android:id="@+id/rowTableFax"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="10sp">

            <TextView
                android:id="@+id/textViewLabelDate"
                android:layout_width="match_parent"
